{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "allowImportingTsExtensions": true,
    },
    "include": ["./src/**/*.ts"],
    "exclude": ["./node_modules"],
    "ts-node": {
        "swc": true
    }
}